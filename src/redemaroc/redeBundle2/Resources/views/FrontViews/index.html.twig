</head>
<body>

<!--START SCROLL TOP BUTTON -->
<a class="scrollToTop" href="#">
    <i class="fa fa-angle-up"></i>
</a>
<!-- END SCROLL TOP BUTTON -->



<!-- Start header section -->
<header id="mu-header" style="position: relative">
    <nav class="navbar navbar-default mu-main-navbar" role="navigation">
        <div class="container" >
            <div class="navbar-header">
                <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                <!-- LOGO -->

                <!--  Text based logo  -->
                <a class="navbar-brand" href="{{ path('rede_homepage') }}"><img src="{{ asset('FrontEnd/assets/img/logo.jpg') }}" alt="Logo img"></a>

                <!--  Image based logo  -->
                <!-- <a class="navbar-brand" href="index.html"><img src="assets/img/logo.png" alt="Logo img"></a>  -->


            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul id="top-menu" class="nav navbar-nav navbar-right mu-main-nav">
                    <li><a href="{{ path('rede_homepage') }}">ACCEUIL</a></li>
                    <li><a href="{{ path('presentation_page') }}">QUI SOMMES-NOUS?</a></li>
                    <li><a href="{{ path('catalogue_page') }}">NOS PRODUITS</a></li>
                    {#<li>
                        #}{#<button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown"> NOS PRODUITS <span class="caret"></span></button>#}{#
                        <a class="li-menu" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">NOS PRODUITS</a>
                        <ul class="dropdown-menu menu-prod">
                            <li><a tabindex="-1" href="{{ path('catalogue_page') }}"> Tous les produits </a></li>
                            {% for t in type %}
                                <li class="dropdown-submenu">
                                    <a class="test" tabindex="-1" href="#">{{ t.type }} <span class="caret"></span></a>
                                    <ul class="dropdown-menu sous-menu-prod">
                                        {%  for produit in t.produit %}
                                            <li><a tabindex="-1" href="{{ path('produit_page', {'id' : produit.id}) }}">{{ produit.titre }}</a></li>
                                        {% endfor %}
                                    </ul>
                                </li>
                            {% endfor %}
                        </ul>
                    </li>#}
                    <li><a href="{{ path('references_page') }}">NOS REFERENCES</a></li>
                    <li><a href="{{ path('galerie_page') }}">GALERIE</a></li>
                    <li><a href="{{ path('documentation_page') }}">DOCUMENTATION</a></li>
                    <li><a href="{{ path('devis_page') }}">DEMANDER DEVIS</a></li>
                    <li><a href="#">E-COMMERCE</a></li>
                    <li><a href="{{ path('rede_homepage') }}#mu-contact">CONTACT</a></li>
                    {#<form class="navbar-form navbar-right" method="get" action="{{ path('produit_recherche') }}">
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Search">
                        </div>
                        <button type="submit" ><i class="fa fa-search"></i></button>
                    </form>#}
                    {#{% render(controller('redeBundle:Default:recherche')) %}#}
                </ul>
            </div>
            <div class="col-md-1 col-sm-1 col-xs-1">
                <button type="button" name="button" id="search-icone"><i class="glyphicon glyphicon-search"></i></button>
            </div>

            <div class="col-md-1 col-sm-1 col-xs-1">
                <button type="button" name="button" id="panier-icone"><i class="glyphicon glyphicon-shopping-cart"></i></button>
            </div>
            {% render(controller('redeBundle:Default:recherche')) %}
            {% render(controller('redeBundle:Default:panier')) %}


        </div>
    </nav>

</header>
<!-- End header section -->


{% block slider %}
{% render(controller('redeBundle:Default:firstlevel'))%}
{% endblock %}

{#{% for p in produit %}
    <h1>{{ p.titre }}</h1>
    <p>{{ p.presentation|raw }}</p>
{% endfor %}#}

{% block body %}



    {% for h in home %}
        <!-- Start petit Mot -->
        <section id="mu-restaurant-menu">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="mu-chef-area">

                            <div class="mu-title">
                                <span class="mu-subtitle"> {{ h.titre }} </span>
                                <h2> {{ h.titresuite }}</h2>
                            </div>

                            <div class="mu-chef-content">
                                <p> {{ h.texte | striptags }} </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- End petit Mot -->


        <!-- Start Counter Section -->
        <section id="mu-counter">
            <div class="mu-counter-overlay">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="mu-counter-area">

                                <ul class="mu-counter-nav">

                                    {#<li class="col-md-3 col-sm-3 col-xs-12">#}
                                    {#<div class="mu-single-counter">#}
                                    {#<span>Fresh</span>#}
                                    {#<h3><span class="counter-value" data-count="150">0</span><sup>+</sup></h3>#}
                                    {#<p>Breakfast Items</p>#}
                                    {#</div>#}
                                    {#</li>#}

                                    <li class="col-md-4 col-sm-4 col-xs-12">
                                        <div class="mu-single-counter">
                                            {#<span>Delicious</span>#}
                                            <h3><span class="counter-value" data-count="{{ h.val1 }}">0</span><sup>+</sup></h3>
                                            <p> {{ h.texte1 }} </p>
                                        </div>
                                    </li>

                                    <li class="col-md-4 col-sm-4 col-xs-12">
                                        <div class="mu-single-counter">
                                            {#<span>Hot</span>#}
                                            <h3><span class="counter-value" data-count="{{ h.val2 }}">0</span><sup>+</sup></h3>
                                            <p> {{ h.texte2 }} </p>
                                        </div>
                                    </li>

                                    <li class="col-md-4 col-sm-4 col-xs-12">
                                        <div class="mu-single-counter">

                                            <h3><span class="counter-value" data-count="{{ h.val3 }}">0</span><sup>+</sup></h3>
                                            <p> {{ h.texte3 }} </p>
                                            {#<span>Satisfied</span>#}
                                        </div>
                                    </li>

                                </ul>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- End Counter Section -->


        <!-- Start nos references -->
        <section id="mu-reference">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="mu-chef-area">

                            <div class="mu-title">
                                <span class="mu-subtitle"> Une sélection </span>
                                <h2> de nos Références </h2>
                            </div>


                            <div class="mu-chef-content">
                                <ul class="mu-chef-nav">
                                    {% for r in refer %}
                                        <li>
                                            <div class="mu-single-chef">
                                                <figure class="mu-single-chef-img">
                                                    <img src="{{ asset('uploads/references/' ~ r.path) }}" alt="chef img">
                                                </figure>
                                                <div class="mu-single-chef-info">
                                                    <h4>{{ r.quoi }}</h4>
                                                    <span>{{ r.ou }}</span>
                                                </div>

                                            </div>
                                        </li>
                                    {% endfor %}

                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- End nos references -->




        {% render(controller('redeBundle:contact:add'))%}


        <!-- Start Map section -->
        <section id="mu-map">
            {#<iframe width="425" height="350" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://www.openstreetmap.org/export/embed.html?bbox=-7.589557170867921%2C33.580347676945%2C-7.5788283348083505%2C33.58690814811236&amp;layer=mapnik" style="border: 1px solid black"></iframe><br/><small><a href="https://www.openstreetmap.org/#map=17/33.58363/-7.58419&amp;layers=N">Afficher une carte plus grande</a></small>#}
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3323.843421779857!2d-7.585684684538351!3d33.58341444973695!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0xda7cd3c32499457%3A0xf4245cf7e8b297f3!2sREDEMAROC!5e0!3m2!1sen!2sma!4v1541435065586" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
        </section>
        <!-- End Map section -->
    {% endfor %}
{% endblock %}

{% for h in home %}
<!-- Start Footer -->
<footer id="mu-footer">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="mu-footer-area">
                    <div class="mu-footer-social">
                        <a href="{{ h.facebook }}" target="_blank"><span class="fa fa-facebook"></span></a>
                        {#<a href="#"><span class="fa fa-twitter"></span></a>#}
                        {#<a href="#"><span class="fa fa-google-plus"></span></a>#}
                        <a href="{{ h.linkedin }}" target="_blank"><span class="fa fa-linkedin"></span></a>
                        <a href="{{ h.youtube }}" target="_blank"><span class="fa fa-youtube"></span></a>
                    </div>
                    <div class="mu-footer-copyright">
                        <p>&copy; Copyright <a rel="nofollow" href="https://www.telecontact.ma" target="_blank">TELECONTACT</a>. </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</footer>
<!-- End Footer -->
{% endfor %}

